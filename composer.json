{
    "name": "lsp/laravel-simple-pattern",
    "type": "library",
    "description": "A generator for Laravel Design Pattern.",
    "keywords": [
        "design pattern",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
          "name": "Chun Yeung",
          "email": "boyz357159@gmail.com",
          "role": "Developer"
        }
      ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^5.7"
    },
    "autoload": {
        "psr-4": {
          "LaravelSimplePattern\\": "src/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "LaravelSimplePattern\\lspServiceProvider"
          ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}